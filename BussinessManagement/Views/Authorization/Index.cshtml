@using BussinessManagement.Models;
@model IEnumerable<MemberType>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    WebGrid grid = new WebGrid(source: Model, rowsPerPage: 5);
}
@grid.GetHtml(tableStyle: "gridTable",
    headerStyle: "gridHeader",
    footerStyle: "gridFooter",
    rowStyle: "gridRows",
    alternatingRowStyle: "gridAltRows",
    columns: grid.Columns(
    grid.Column("IDTypeMember", "ID"),
    grid.Column("NameType", "Name"),
    grid.Column(format: p => @Html.ActionLink("Authorization", "Authorization", new { id = p.IDTypeMember }))

))
<link href="~/Content/admin/css/style-grid.css" rel="stylesheet" />
